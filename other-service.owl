    <!-- http://dictionary.casrai.org/ontology/rai/other-service/CommitteeMembershipRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/CommitteeMembershipRole">
        <rdfs:label xml:lang="en">Committee Membership</rdfs:label>
        <rdfs:label xml:lang="fr">Participation à des comité</rdfs:label>
        <skos:definition>Services contributed as part of a group elected or appointed to perform such services but not directly related to the person&apos;s research activities.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/CommunityServiceRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/CommunityServiceRole">
        <rdfs:label xml:lang="en">Community Service</rdfs:label>
        <rdfs:label xml:lang="fr">Service dans la collectivité</rdfs:label>
        <skos:definition>Services contributed, unpaid, on behalf of one’s locality, social, religious, occupational, or other group sharing common characteristics or interests, but not directly related to the person&apos;s research activities.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/Course-basedDegreeSupervision -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/Course-basedDegreeSupervision">
        <rdfs:label xml:lang="en">Course-based Degree Supervision</rdfs:label>
        <rdfs:label xml:lang="fr">Supervisions de diplôme à partir de cour</rdfs:label>
        <skos:definition>Services contributed in instances of overseeing the productivity and progress of a student reporting directly to the person, who is undertaking research activity, usually in pursuit of academic credit.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/CourseDeveloped -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/CourseDeveloped">
        <rdfs:label xml:lang="en">Course Developed</rdfs:label>
        <rdfs:label xml:lang="fr">Cours élaboré</rdfs:label>
        <skos:definition>Contributions in the development of courses/modules for training or teaching purposes.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/CourseTaught -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/CourseTaught">
        <rdfs:label xml:lang="en">Course Taught</rdfs:label>
        <rdfs:label xml:lang="fr">Cours enseigné</rdfs:label>
        <skos:definition>Services contributed in the form of courses taught at academic institutions with which the person is currently, or has in the past been, affiliated.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/Editorial -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/Editorial">
        <rdfs:label xml:lang="en">Editorial</rdfs:label>
        <rdfs:label xml:lang="fr">ditorial</rdfs:label>
        <skos:definition>Contribution in the provision of an opinion statement regarding a certain topic.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/EmployeeSupervision -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/EmployeeSupervision">
        <rdfs:label xml:lang="en">Employee Supervision</rdfs:label>
        <rdfs:label xml:lang="fr">Supervisions d’employé</rdfs:label>
        <skos:definition>Services contributed in instances of overseeing the productivity and progress of an employee reporting directly to the person.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/EventAdministrationRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/EventAdministrationRole">
        <rdfs:label xml:lang="en">Event Administration</rdfs:label>
        <rdfs:label xml:lang="fr">Administration d’événement</rdfs:label>
        <skos:definition>Services contributed in the planning, organization, coordination and staging of a public event.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/EventParticipationRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/EventParticipationRole">
        <rdfs:label xml:lang="en">Event Participation</rdfs:label>
        <rdfs:label xml:lang="fr">Participation à un événement</rdfs:label>
        <skos:definition>Services contributed in taking part in an event.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/MembershipRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/MembershipRole">
        <rdfs:label xml:lang="en">Membership</rdfs:label>
        <rdfs:label xml:lang="fr">Appartenance</rdfs:label>
        <skos:definition>Services contributed as part of a society, community, or other body.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/Non-researchPresentation -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/Non-researchPresentation">
        <rdfs:label xml:lang="en">Non-research Presentation</rdfs:label>
        <rdfs:label xml:lang="fr">Exposés non liés à la recherche</rdfs:label>
        <skos:definition>Contributions of presentations to groups of people, not directly related to the person&apos;s research activities, and not delivered as part of a formal course of study.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/OfficeHolderRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/OfficeHolderRole">
        <rdfs:label xml:lang="en">Office Held</rdfs:label>
        <rdfs:label xml:lang="fr">Charges détenue</rdfs:label>
        <skos:definition>Services contributed in a position of duty or function.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/other-service/ProgramDevelopmentRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/other-service/ProgramDevelopmentRole">
        <rdfs:label xml:lang="en">Program Developed</rdfs:label>
        <rdfs:label xml:lang="fr">Programmes élaboré</rdfs:label>
        <skos:definition>Contribution in cases of development of an entire course of study.</skos:definition>
    </owl:Class>
