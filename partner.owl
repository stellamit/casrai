       <skos:definition>Information about all past, current and pending funding associated with this research activity.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/partner/Partner">
       <rdfs:label xml:lang="en">Partner</rdfs:label>
       <skos:definition>tbd</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/partner/In-kindFunding">
       <rdfs:label xml:lang="en">In-kind Funding</rdfs:label>
       <skos:definition>Information about all past, current and pending funding associated with this research activity.</skos:definition>
     </owl:Class>

</rdf:RDF>