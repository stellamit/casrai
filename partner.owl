     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
     <owl:Ontology rdf:about="http://dictionary.casrai.org/ontology/rai/partner/">
        <rdfs:comment>http://dictionary.casrai.org/research-activity-profile-draft/partners</rdfs:comment>
        <owl:versionInfo>2012</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://dictionary.casrai.org/ontology/rai/partner/2012/"/>
     </owl:Ontology>
  
     <owl:AnnotationProperty rdf:about="&skos;definition"/>
 
         

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/partner/CashFunding">
