<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF xmlns="http://dictionary.casrai.org/ontology/rai/education/"
     xml:base="http://dictionary.casrai.org/ontology/rai/education/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
     <owl:Ontology rdf:about="http://dictionary.casrai.org/ontology/rai/education/">
        <rdfs:comment>http://dictionary.casrai.org/research-personnel-profile/education</rdfs:comment>
        <owl:versionInfo>2012</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://dictionary.casrai.org/ontology/rai/education/2012/"/>
     </owl:Ontology>
  
     <owl:AnnotationProperty rdf:about="&skos;definition"/>
 
         

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/education/Degree">
       <rdfs:label xml:lang="en">Degree</rdfs:label>
       <rdfs:label xml:lang="fr">Diplôme</rdfs:label>
       <skos:definition>An academic title conferred by universities and colleges as an indication of the completion of a course of study, or as an honorary recognition of achievement.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/education/Supervisor">
       <rdfs:label xml:lang="en">Supervisor</rdfs:label>
       <rdfs:label xml:lang="fr">Directeur</rdfs:label>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/education/ProfessionalDesignation">
       <rdfs:label xml:lang="en">Professional Designation</rdfs:label>
       <rdfs:label xml:lang="fr">Titres professionnel</rdfs:label>
       <skos:definition>A designation earned to assure qualification to perform a job or task.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/education/OtherStudy">
       <rdfs:label xml:lang="en">Other Study</rdfs:label>
       <rdfs:label xml:lang="fr">Autres étude</rdfs:label>
       <skos:definition>Other training/courses undertaken by the person.</skos:definition>
     </owl:Class>

</rdf:RDF>