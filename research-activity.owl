<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF xmlns="http://dictionary.casrai.org/ontology/rai/research-activity/"
     xml:base="http://dictionary.casrai.org/ontology/rai/research-activity/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
     <owl:Ontology rdf:about="http://dictionary.casrai.org/ontology/rai/research-activity/">
        <rdfs:comment>http://dictionary.casrai.org/research-activity-profile-draft/details</rdfs:comment>
        <owl:versionInfo>2012</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://dictionary.casrai.org/ontology/rai/research-activity/2012/"/>
     </owl:Ontology>
  
     <owl:AnnotationProperty rdf:about="&skos;definition"/>
 
         

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-activity/Risk">
       <rdfs:label xml:lang="en">Risk</rdfs:label>
       <skos:definition>Identified risks related to the research activity.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-activity/Overview">
       <rdfs:label xml:lang="en">Overview</rdfs:label>
       <skos:definition>Information about the Objectives, Scope and Methodology of the research activity.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-activity/Budget">
       <rdfs:label xml:lang="en">Budget</rdfs:label>
       <skos:definition>Information detailing the projected revenues and expenditures for the Research Activity.</skos:definition>
     </owl:Class>

     <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-activity/Milestone">
       <rdfs:label xml:lang="en">Milestone</rdfs:label>
       <skos:definition>Information about scientific milestones projected to be reached during the course of this
activity.</skos:definition>
     </owl:Class>

</rdf:RDF>