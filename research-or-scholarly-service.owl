    <!-- http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/ConferenceReviewingOrRefereeing -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/ConferenceReviewingOrRefereeing">
