    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/MentorRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/MentorRole">
        <rdfs:label xml:lang="en">Mentoring</rdfs:label>
        <rdfs:label xml:lang="fr">Mentorat</rdfs:label>
        <skos:definition>Services contributed, in order to provide counsel, guidance, and instruction to a protege.</skos:definition>
    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/PromotionOrTenureAssessment -->

