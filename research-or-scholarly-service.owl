    </owl:Class>
    


    <!-- http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/ConsultantAdvisorRole -->

    <owl:Class rdf:about="http://dictionary.casrai.org/ontology/rai/research-or-scholarly-service/ConsultantAdvisorRole">
